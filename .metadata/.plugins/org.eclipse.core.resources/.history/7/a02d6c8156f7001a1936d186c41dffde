	dataset.addRow(new Array("Veracruz", "MX"));
	dataset.addRow(new Array("Buenos Aires", "AR"));
	dataset.addRow(new Array("Santiago", "CL"));
	dataset.addRow(new Array("Montevidéu", "UY"));

